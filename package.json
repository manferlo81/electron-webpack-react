{
  "name": "electron-webpack-react",
  "version": "0.0.0",
  "description": "A simple boilerplate using Electron, electron-webpack, React, Typescript & Sass",
  "scripts": {
    "clean": "rm -rf dist app",
    "postinstall": "electron-webpack dll",
    "dev": "electron-webpack dev",
    "dll": "electron-webpack dll",
    "compile": "electron-webpack",
    "build:dir": "electron-webpack && electron-builder --dir -c.compression store -c.mac.identity null -c.asar false",
    "build": "electron-webpack && electron-builder"
  },
  "electronWebpack": {
    "title": "Electron + Webpack + React",
    "renderer": {
      "dll": [
        "react",
        "react-dom"
      ]
    }
  },
  "build": {
    "productName": "electron-react",
    "directories": {
      "output": "app"
    },
    "win": {
      "icon": "icons/win.ico"
    }
  },
  "dependencies": {
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.8.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^8.0.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "license": "MIT"
}
